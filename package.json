		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"fetch-jsonp": "^1.3.0",
		"gh-pages": "^6.1.1",
		"globals": "^15.4.0",
		"js-yaml": "^4.1.0",
